{
  "name": "@khatastroffik/figleteer",
  "version": "1.0.0",
  "description": "A FIGlet library written in Typescript usable by ESM, CJS and TS clients and a CLI FIGlet generator",
  "author": {
    "name": "Loïs Bégué",
    "url": "https://www.khatastroffik.net"
  },
  "repository": {
    "url": "https://github.com/khatastroffik/figleteer.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/khatastroffik/figleteer/issues"
  },
  "homepage": "https://www.khatastroffik.net",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=13.x",
    "npm": ">=6.x"
  },
  "directories": {
    "doc": "doc"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "main": "dist/figleteer.js",
  "types": "types",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "devtest": "jest --watch --verbose",
    "devbuild": "tsc --watch",
    "build": "tsc",
    "lint": "eslint -f pretty src",
    "checkpkg": "pkgfiles"
  },
  "keywords": [
    "FIGlet",
    "FIGfonts",
    "ASCII",
    "ASCII text",
    "ASCII art",
    "Typescript",
    "ESM",
    "ECMAScript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-deprecation": "^1.1.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-notice": "^0.9.10",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "jest-mock-process": "^1.4.0",
    "pkgfiles": "^2.3.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
